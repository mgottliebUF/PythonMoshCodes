#Dictionary Practice 
phone = input("Phone: ")
digits_mapping = {
    "0": "Zero"
    "1": "One", 
    "2": "Two",
    "3": "Three",
    "4": "Four",
    "5": "Five",
    "6": "Six",
    "7": "Seven",
    "8": "Eight",
    "9": "Nine"
}

output = "" #Initializes empty string
for ch in phone: #Traverses the user input "phone"
    output += digits_mapping.get(ch, "!") + " " #Maps the digits  
print(output)
